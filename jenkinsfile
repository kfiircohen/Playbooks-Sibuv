pipeline {
    agent any
	
    stages {
        stage('Build') {
            steps {
                echo 'Building..'
            }
        }
	stage('Mvdir') {
	    steps {
	    	sh '''|
		       echo "Changing name of Dir..."
		       mv sibuv.txt sibuvi.txt
		    '''
	    }
	}
        stage('Test') {
            steps {
                echo 'Testing..'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying....'
            }
        }
    }
}
